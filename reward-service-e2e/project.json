{
  "name": "reward-service-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["reward-service"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "reward-service-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["reward-service:build", "reward-service:serve"]
    }
  }
}
