{
  "name": "notification-gateway-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["notification-gateway"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "notification-gateway-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["notification-gateway:build", "notification-gateway:serve"]
    }
  }
}
